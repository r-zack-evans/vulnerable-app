{
  "name": "vulnerable-demo-app",
  "version": "1.0.0",
  "description": "A demonstration application with intentional security vulnerabilities",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "dev:api": "nodemon src/index.ts",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:server": "tsc && cp -r src/public dist/",
    "build:client": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.vue",
    "test": "jest",
    "seed": "ts-node src/seeds/run.ts",
    "seed:users": "ts-node src/seeds/seed-users.ts",
    "seed:db": "ts-node src/seeds/seed-db.ts",
    "seed:tasks": "ts-node src/seeds/seed-tasks.ts"
  },
  "keywords": [
    "security",
    "vulnerabilities",
    "owasp",
    "demonstration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pinia": "^3.0.2",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.17",
    "vite": "^6.3.5",
    "vue": "^3.5.13",
    "vue-loader": "^17.4.2",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "concurrently": "^9.1.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
